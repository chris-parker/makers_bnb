<body>
  <h1>My Spaces</h1>

  <% @spaces.each do |space| %>
    <li>
      <%= space.name %>
      <%= space.description %>
      <%= space.price %>
    </li>
  <% end %>

  <form class="add_space" action="/new/space" method="get">
    <input type="submit" value="add_space">
  </form>
  <form class="all_spaces" action="/spaces" method="get">
    <input type="submit" value="all_spaces">
  </form>
</body>
